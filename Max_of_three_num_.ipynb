        "        largest = a \r\n",
        "  \r\n",
        "    elif (b >= a) and (b >= c): \r\n",
        "        largest = b \r\n",
        "    else: \r\n",
        "        largest = c \r\n",
        "          \r\n",
        "    return largest \r\n",
        "  \r\n",
        "  \r\n",
        "# Driven code  \r\n",
